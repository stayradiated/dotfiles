[calendars]

  [[runn]]
    path = ~/dotfiles/apps/vdirsyncer/calendars/runn/*
    type = discover
    color = light blue
    readonly = True

  [[personal]]
    path = ~/dotfiles/apps/vdirsyncer/calendars/personal/*
    type = discover
    color = dark green

[locale]
timeformat = %H:%M
dateformat = %Y-%m-%d
longdateformat = %Y-%m-%d
datetimeformat = %Y-%m-%d %H:%M
